    "@realmjs/account-client": "^1.0.0",
    "@realmjs/react-user": "^1.0.1",
    "@realmjs/xhttp-request": "^1.3.1",
    "express-api-builder": "^1.3.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
